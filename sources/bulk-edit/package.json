{
  "name": "craftercms-plugin-bulk-edit-site",
  "version": "1.0.0",
  "description": "A Crafter CMS 4 plugin to execute bulk content edit",
  "author": "phuongnq",
  "license": "MIT",
  "repository": "phuongnq/craftercms-plugin-bulk-edit-site",
  "main": "dist/index.js",
  "umd": "dist/index.umd.js",
  "module": "dist/index.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "commit": "git add ../../config/studio/plugins/js/org/craftercms/plugin/sidebar/bulkedit/index.js && git commit -m 'development'",
    "watch": "rollup --config rollup.config.js -w",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "i18n": "run-s i18n:extract i18n:compile i18n:manage",
    "i18n:extract": "formatjs extract --extract-source-location true --out-file ./src/i18n/messages/extract.json ./src/**.{tsx,ts}",
    "i18n:compile": "formatjs compile --out-file ./src/i18n/translations/en.json ./src/i18n/messages/extract.json",
    "i18n:manage": "node manageTranslations.js"
  },
  "peerDependencies": {
    "@emotion/css": "^11.1.3",
    "@mui/material": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@formatjs/cli": "^2.13.12",
    "@mui/material": "^5.0.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.10.1",
    "react-intl-translations-manager": "^5.0.3",
    "react-scripts": "^4.0.0",
    "rollup": "^2.33.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commit": "^0.0.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-replace-imports-with-vars": "^1.0.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@mui/icons-material": "^5.0.1",
    "@mui/styles": "^5.0.1",
    "@mui/x-data-grid": "^5.0.0-beta.4",
    "react-draggable": "^4.4.4"
  }
}
